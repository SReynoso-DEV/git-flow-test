{
  "baseUrl": "https://github.com/tecnologiadigital/velocity",
  "ignorePatterns": [
    {
      "pattern": "^https://hadenlabs.com"
    },
    {
      "pattern": "^https://github.com/tecnologiadigital/velocity/releases.*$"
    },
    {
      "pattern": "^https://github.com/tecnologiadigital/velocity/pulls.*$"
    },
    {
      "pattern": "^https://github.com/tecnologiadigital/velocity/actions.*$"
    },
    {
      "pattern": "^https://github.com/tecnologiadigital/velocity/issues.*$"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/docs",
      "replacement": "{{BASEURL}}/docs"
    }
  ],
  "timeout": "20s",
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "aliveStatusCodes": [200, 206]
}
